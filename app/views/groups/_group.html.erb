<div id="<%= dom_id group %>">
  <p>
    <strong>Name:</strong>
    <%= group.name %>
  </p>

  <p>
    <strong>City:</strong>
    <%= group.city %>
  </p>

  <p>
    <strong>Location:</strong>
    <%= group.location_id %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= group.description %>
  </p>

  <p>
    <strong>Private:</strong>
    <%= group.private %>
  </p>

  <p>
    <strong>Event setting:</strong>
    <%= group.event_setting_id %>
  </p>

</div>
