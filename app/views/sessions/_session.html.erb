<div id="<%= dom_id session %>">
  <p>
    <strong>Game:</strong>
    <%= session.game_id %>
  </p>

  <p>
    <strong>Event:</strong>
    <%= session.event_id %>
  </p>

  <p>
    <strong>User:</strong>
    <%= session.user_id %>
  </p>

  <p>
    <strong>Slot number:</strong>
    <%= session.slot_number %>
  </p>

  <p>
    <strong>Max slots:</strong>
    <%= session.max_slots %>
  </p>

</div>
